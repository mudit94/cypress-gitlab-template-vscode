{
  "name": "cypress-setup",
  "displayName": "cypress-setup",
  "description": "A basic template for initializing a cypress framework",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cypress-setup.cypress-allure-gitlabsetup",
        "title": "Cypress framework initialization with Gitlab CI and Allure setup"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "compile": "node ./node_modules/vscode/bin/compile",
    "pretest": "pnpm run lint",
    "test": "node ./test/runTest.js",
    "postinstall": "npm install allure-commandline --save-dev",
    "vsce": "vsce package"

  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@vscode/test-electron": "^2.3.2",
    "allure-commandline": "^2.22.4",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "path": "^0.12.7",
    "typescript": "^5.1.3"
  }
}
